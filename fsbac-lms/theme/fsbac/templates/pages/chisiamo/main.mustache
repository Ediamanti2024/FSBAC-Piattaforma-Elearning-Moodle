
<div class="fsbac-header-page">
    <div class="container">
        <div class="fsbac-header-section-text">
            <p class='infotext'>{{#str}} chisiamo.header_info , theme_fsbac {{/str}}</p>
            <h1 class="title">{{#str}} chisiamo.header_title , theme_fsbac {{/str}}</h1>
        </div>
        <div class="fsbac-header-section-image fsbac-header-section-video">
        <video  controls>
            <source src="{{config.wwwroot}}/theme/fsbac/pix/chisiamo/chisiamo.mp4" type="video/mp4">

            Your browser does not support the video tag.
        </video>
        </div>
    </div>
   
</div>
<div class="container">
    <section class="section-todo">
       {{< theme_fsbac/components/title-section}}
